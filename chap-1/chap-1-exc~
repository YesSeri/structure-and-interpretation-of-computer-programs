#lang racket
(define (square x)(* x x))
(define (sum-of-squares x y)(+ (square x) (square y)))
(define a 3)
(define b (+ a 1))
(define c  (/
  (+ 5 4
     (- 2
        (- 3(+ 6(/ 4 6)))))
  (* 3
     (- 6 2)
        (- 2 7))))
(define (sum-of-largest-squares x y z)
  (cond ((and (< x y)(< x z)(sum-of-squares y z)))
        ((< y z)(sum-of-squares z x))
        (else (sum-of-squares y x))))
